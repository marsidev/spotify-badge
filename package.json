{
  "name": "spotify-badge",
  "version": "1.1.0",
  "license": "MIT",
  "packageManager": "pnpm@7.32.4",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "vercel dev",
    "deploy": "vercel --prod",
    "type-check": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "isomorphic-unfetch": "^4.0.2",
    "preact": "^10.13.2",
    "preact-render-to-string": "^5.2.6"
  },
  "devDependencies": {
    "@types/node": "^18.16.8",
    "@types/spotify-api": "^0.0.21",
    "@vercel/node": "^2.14.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "vercel": "^25.2.3"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "vercel dev",
    "watch": [
      "api/*",
      "components/*",
      "consts.ts",
      "lib/*"
    ]
  }
}
